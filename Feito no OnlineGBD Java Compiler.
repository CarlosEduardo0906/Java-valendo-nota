class Main {
    public static void main(String[] args) {
        Taxi t = new Taxi();
        t.setAssentos(4);
        System.out.println("A quantidade de assentos no taxi é " + t.getAssentos()+"\n");
        t.setkmRodados(250);
        System.out.println("Os kilometros rodados são " + t.getkmRodados()+"\n");
        t.setVelmax(150);
        System.out.println("A velocidade maxima do veiculo e " + t.getVelmax()+"\n");
        t.setPassageiros(4);
        System.out.println("A quantidade total de passageiros no taxi é " + t.getPassageiros()+"\n");
        t.setValor(35);
        System.out.println("O valor da corrida é de " + t.getValor()+"\n");
        t.setRota("Shopping RioMar");
        System.out.println("O destino dá viagem é " + t.getRota()+"\n");
        System.out.println("Agostinho Carrara aprova");
    }
}

class Taxi{
   private int assentos;
   private int kmrodados;
   private int velMax;
   private int passageiros;
   private int valor;
   private String rota;
   
   public int getAssentos(){
       return this.assentos;
   }
   
   public void setAssentos(int assentos){
       this.assentos = assentos;
       
   }
   public int getkmRodados(){
       return this.kmrodados;
}
   public void setkmRodados(int kmrodados){
        this.kmrodados = kmrodados;
    
  }
   public int getVelmax(){
      return this.velMax;
  }
  
   public void setVelmax(int velMax){
       this.velMax = velMax;
   }
   public int getPassageiros(){
       return this.passageiros;
   }
   public void setPassageiros(int passageiros){
       this.passageiros = passageiros;
   }
   public int getValor(){
       return this.valor;
   }
  public void setValor(int valor){
      this.valor = valor;
  }
  public String getRota(){
      return this.rota;
  }
  public void setRota(String rota){
      this.rota = rota;
  }
}
